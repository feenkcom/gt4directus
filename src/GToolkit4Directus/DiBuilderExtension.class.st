Class {
	#name : 'DiBuilderExtension',
	#superclass : 'Object',
	#instVars : [
		'builder'
	],
	#category : 'GToolkit4Directus-Builder'
}

{ #category : 'accessing' }
DiBuilderExtension >> builder [
	^ builder
]

{ #category : 'accessing' }
DiBuilderExtension >> builder: anObject [
	builder := anObject
]
